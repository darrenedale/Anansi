<?xml version="1.0" encoding="UTF-8"?>
<webserver>
    <documentroot platform="linux">/home/darren/Public</documentroot>
    <bindaddress>172.16.1.103</bindaddress>
    <bindport>1900</bindport>
    <defaultconnectionpolicy>
        <connectionpolicy>Reject</connectionpolicy>
    </defaultconnectionpolicy>
    <defaultmimetype>
        <mimetype>application/octet-stream</mimetype>
    </defaultmimetype>
    <defaultmimetypeaction>
        <webserveraction>Ignore</webserveraction>
    </defaultmimetypeaction>
    <allowdirectorylistings>true</allowdirectorylistings>
    <ignorehiddenfiles>true</ignorehiddenfiles>
    <ipconnectionpolicylist>
        <ipconnectionpolicy>
            <ipaddress>172.16.1.105</ipaddress>
            <connectionpolicy>None</connectionpolicy>
        </ipconnectionpolicy>
        <ipconnectionpolicy>
            <ipaddress>172.16.1.103</ipaddress>
            <connectionpolicy>Accept</connectionpolicy>
        </ipconnectionpolicy>
    </ipconnectionpolicylist>
    <extensionmimetypelist>
        <extensionmimetype>
