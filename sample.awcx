            <webserveraction>Forbid</webserveraction>
        </mimetypeaction>
    </mimetypeactionlist>
    <mimetypecgilist>
        <mimetypecgi>
            <mimetype>application/x-php</mimetype>
            <cgiexecutable>/usr/bin/php-cgi</cgiexecutable>
        </mimetypecgi>
    </mimetypecgilist>
