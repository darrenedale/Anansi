            <extension>css</extension>
            <mimetype>text/css</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>gif</extension>
            <mimetype>image/gif</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>h</extension>
            <mimetype>text/x-c-header</mimetype>
            <mimetype>text/x-cpp-header</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>hpp</extension>
            <mimetype>text/x-cpp-header</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>htm</extension>
            <mimetype>text/html</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>html</extension>
            <mimetype>text/html</mimetype>
            <mimetype>text/plain</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>ico</extension>
            <mimetype>image/x-ico</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>jpeg</extension>
            <mimetype>image/jpeg</mimetype>
        </extensionmimetype>
        <extensionmimetype>
            <extension>jpg</extension>
            <mimetype>image/jpeg</mimetype>
        </extensionmimetype>
        <extensionmimetype>
